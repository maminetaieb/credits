<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane fx:id="createClientPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.app.CreateClient">
   <children>
      <Label layoutX="70.0" layoutY="91.0" text="Nom Prénom :">
         <font>
            <Font name="Arial Bold" size="20.0" />
         </font>
      </Label>
      <Label layoutX="70.0" layoutY="153.0" text="Numéro Téléphone :">
         <font>
            <Font name="Arial Bold" size="20.0" />
         </font>
      </Label>
      <Label layoutX="70.0" layoutY="212.0" text="Max Crédit :">
         <font>
            <Font name="Arial Bold" size="20.0" />
         </font>
      </Label>
      <TextField fx:id="nomField" layoutX="274.0" layoutY="84.0" onAction="#nom" prefHeight="38.0" prefWidth="207.0" />
      <TextField fx:id="maxField" layoutX="274.0" layoutY="204.0" onAction="#max" prefHeight="38.0" prefWidth="207.0" />
      <TextField fx:id="numTelField" layoutX="274.0" layoutY="146.0" onAction="#numTel" prefHeight="38.0" prefWidth="207.0" />
      <Button fx:id="AjouterClientBtn" layoutX="264.0" layoutY="298.0" mnemonicParsing="false" onAction="#ajouterClient" prefHeight="23.0" prefWidth="101.0" text="Ajouter">
         <font>
            <Font size="19.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
